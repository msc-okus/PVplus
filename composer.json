{
    "type": "project",
    "license": "proprietary",
    "repositories":[
        {"type":"composer","url":"https://repo.koolreport.com"}
    ],
    "require": {
        "php": "^8.1",
        "ext-calendar": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "api-platform/core": "^2.6",
        "babdev/pagerfanta-bundle": "^3.7",
        "beberlei/doctrineextensions": "^1.3",
        "chromeheadlessio/php-client": "^1.3",
        "composer/package-versions-deprecated": "1.11.99.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.9",
        "friendsofsymfony/ckeditor-bundle": "^2.3",
        "gedmo/doctrine-extensions": "^3.6",
        "hisune/echarts-php": "^1.0",
        "jblond/twig-trans": "^1.0",
        "knplabs/knp-paginator-bundle": "^5.2",
        "knplabs/knp-snappy-bundle": "^1.9",
        "league/html-to-markdown": "^4.10",
        "liip/imagine-bundle": "^2.8",
        "nelmio/cors-bundle": "^2.1",
        "nuzkito/chrome-html-to-pdf": "^2.0",
        "omines/datatables-bundle": "*",
        "pagerfanta/doctrine-orm-adapter": "^3.7",
        "pagerfanta/twig": "^3.7",
        "php-http/guzzle6-adapter": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpoffice/phpspreadsheet": "^1.20",
        "phpstan/phpdoc-parser": "^1.16",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.3",
        "shuchkin/simplexlsx": "^0.8.33",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "6.0.*",
        "symfony/console": "6.0.*",
        "symfony/debug-bundle": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/expression-language": "6.0.*",
        "symfony/finder": "6.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "6.0.*",
        "symfony/framework-bundle": "6.0.*",
        "symfony/mailer": "6.0.*",
        "symfony/maker-bundle": "^1.43",
        "symfony/messenger": "6.0.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/process": "6.0.*",
        "symfony/property-access": "6.0.*",
        "symfony/property-info": "6.0.*",
        "symfony/proxy-manager-bridge": "6.0.*",
        "symfony/runtime": "6.0.*",
        "symfony/security-bundle": "6.0.*",
        "symfony/serializer": "6.0.*",
        "symfony/stimulus-bundle": "*",
        "symfony/twig-bundle": "6.0.*",
        "symfony/ux-chartjs": "^2.0",
        "symfony/validator": "6.0.*",
        "symfony/var-dumper": "6.0.*",
        "symfony/webpack-encore-bundle": "^1.13",
        "symfony/yaml": "6.0.*",
        "tecnickcom/tc-lib-pdf": "dev-main",
        "twig/cssinliner-extra": "^3.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/inky-extra": "^3.0",
        "twig/intl-extra": "^3.0",
        "twig/string-extra": "^3.4",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "rector/rector": "^0.18.1",
        "symfony/stopwatch": "^6.0",
        "symfony/web-profiler-bundle": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "php -dmemory_limit=3G bin/console cache:clear": "script",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}

                <th>X</th>
                <th>Y</th>
                <th>Z</th>
                <!--<th>AA</th>-->
                <th>AB</th>
                <th>AC</th>
                <th>AC1</th>
                <th>AD</th>
                <!--<th>AE</th>-->
                <!--<th>AF</th>-->
                <th class="border-right-2">AG</th>
                <th>AH</th>
                <th>AI</th>
                <th>AJ</th>
                <th>AK</th>
                <th>AL</th>
            </tr>
            <tr>
                <th></th>
                <th class="border-right-2"></th>
                <th colspan="9" class="border-right-2">Design_M expected / predicted</th>

                <th colspan="10" class="border-right-2">Measured</th>
                <th colspan="5">Data Analysis with risk forecast & FT</th>
            </tr>
            <tr>
                <th style="min-width: 45px">Month</th>
                <!--<th>Days in Month</th>-->
                <th class="border-right-2">Days</th>

                <!--<th>Irradiation [kWh/m&sup2;]</th>-->
                <!--<th>reference yield [kWh/kWp]</th>-->
                <th>theoretical energy [kWh]</th>
                <th>Egrid [kWh]</th>
                <!--<th>Specific Yield [kWh/kWp]</th>-->
                <th>PR [%]</th>
                <th>T ambient [°C]</th>
                <th>T array_avg weighted [°C]</th>
                <!--<th>temperature compensation factor FT</th>-->
                <!--<th>effective reference yield [kWh/kWp] with FT</th>-->
                <th>effective theoretical energy [kWh] with FT</th>
                <th>PR month [%] with FT</th>
                <th>PR guara. [%]<br>with FT</th>
                <!--<th>Specific Yield guaranteed [kWh/kWp]</th>-->
                <th class="border-right-2">Egrid guara. [kWh]</th>

                <th>Irradiation [kWh/m&sup2;]</th>
                <!--<th>reference yield [kWh/kWp]</th>-->

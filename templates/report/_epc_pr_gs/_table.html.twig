{% block page %}
    <h3 style="color: {{ anlage.eigner.fontColor2 }};">Monthly Values</h3>
    <table class="basetable monthtable">
        <thead>
            <tr>
                <th>B</th>
                <!--<th>C</th>-->
                <th class="border-right-2">D</th>
                <!--<th>E</th>-->
                <!--<th>F</th>-->
                <th>G</th>
                <th>H</th>
                <!--<th>I</th>-->
                <th>J</th>
                <th>K</th>
                <th>L</th>

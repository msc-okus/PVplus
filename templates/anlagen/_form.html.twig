<style>
    .sk-chase {
        width: 100px;
        height: 100px;
        position: relative;
        animation: sk-chase 2.5s infinite linear both;
        margin: auto;
    }

    .sk-chase-dot {
        width: 50px;
        height: 50px;
        position: absolute;
        left: 0;
        top: 0;
        animation: sk-chase-dot 2.0s infinite ease-in-out both;
    }

    .sk-chase-dot:before {
        content: '';
        display: block;
        width: 15%;
        height: 15%;
        background-color:#8DBE48;
        border-radius: 100%;
        animation: sk-chase-dot-before 2.0s infinite ease-in-out both;
    }

    .sk-chase-dot:nth-child(1) { animation-delay: -2.1s; }
    .sk-chase-dot:nth-child(2) { animation-delay: -2.0s; }
    .sk-chase-dot:nth-child(3) { animation-delay: -1.9s; }
    .sk-chase-dot:nth-child(4) { animation-delay: -1.8s; }
    .sk-chase-dot:nth-child(5) { animation-delay: -1.7s; }
    .sk-chase-dot:nth-child(6) { animation-delay: -1.6s; }
    .sk-chase-dot:nth-child(1):before { animation-delay: -2.1s; }
    .sk-chase-dot:nth-child(2):before { animation-delay: -2.0s; }
    .sk-chase-dot:nth-child(3):before { animation-delay: -1.9s; }
    .sk-chase-dot:nth-child(4):before { animation-delay: -1.8s; }
    .sk-chase-dot:nth-child(5):before { animation-delay: -1.7s; }
    .sk-chase-dot:nth-child(6):before { animation-delay: -1.6s; }

    @keyframes sk-chase {
        100% { transform: rotate(360deg); }
    }

    @keyframes sk-chase-dot {
        80%, 100% { transform: rotate(360deg); }
    }

    @keyframes sk-chase-dot-before {
        50% {
            transform: scale(0.4);
        } 100%, 0% {
              transform: scale(1.0);
          }
        from { background-color: limegreen }
        to   { background-color: darkblue }
    }

</style>

